<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller.ChefFieldController">
   <children>
      <TabPane fx:id="tp" layoutY="-1.0" prefHeight="400.0" prefWidth="700.0" tabClosingPolicy="UNAVAILABLE">
        <tabs>
          <Tab fx:id="ChefMenu" style="-fx-background-color: #abd1b5;" text="Menu">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <TableView fx:id="MenuTable" layoutY="-2.0" prefHeight="372.0" prefWidth="700.0">
                          <columns>
                            <TableColumn fx:id="PatientId" prefWidth="350.0" style="-fx-background-color: #abd1b5;" text="Patient id" />
                            <TableColumn fx:id="PatientMenu" prefWidth="350.0" style="-fx-background-color: #abd1b5;" text="Menu" />
                          </columns>
                        </TableView>
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
          <Tab fx:id="ChefOrder" style="-fx-background-color: #abd1b5;" text="Order field">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <TableView fx:id="OrderTable" layoutY="-2.0" prefHeight="373.0" prefWidth="700.0">
                          <columns>
                            <TableColumn fx:id="NOrder" prefWidth="175.0" style="-fx-background-color: #abd1b5;" text="N.commande" />
                            <TableColumn fx:id="DateOrder" prefWidth="175.0" style="-fx-background-color: #abd1b5;" text="date commande" />
                              <TableColumn fx:id="Order" prefWidth="175.0" style="-fx-background-color: #abd1b5;" text="commande" />
                              <TableColumn fx:id="QuantiteOrder" prefWidth="175.0" style="-fx-background-color: #abd1b5;" text="quantité commandé" />
                          </columns>
                        </TableView>
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
        </tabs>
      </TabPane>
   </children>
</AnchorPane>
