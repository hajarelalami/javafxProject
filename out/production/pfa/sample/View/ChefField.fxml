<AnchorPane maxWidth="900.0" prefHeight="600.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller.ChefFieldController">
    <children>
        <TabPane prefHeight="600.0" prefWidth="900.0">
            <tabs>
                <Tab closable="false" text="Menu">
                    <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                            <children>
                                <TableView fx:id="menushown" layoutX="63.0" layoutY="14.0" prefHeight="335.0" prefWidth="738.0">
                                    <columns>
                                        <TableColumn fx:id="PatientId" prefWidth="175.0" text="patientid" />
                                        <TableColumn fx:id="patientbreakfast" prefWidth="185.0" text="breakfast" />
                                        <TableColumn fx:id="patientLunch" prefWidth="185.0" text="lunch" />
                                        <TableColumn fx:id="patientDinner" prefWidth="185.0" text="dinner" />
                                    </columns>
                                </TableView>
                            </children>
                        </AnchorPane>
                    </content>
                </Tab>
                <Tab closable="false" text="Menu details">
                    <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                            <children>
                                <ComboBox fx:id="menunames" layoutX="354.0" layoutY="65.0" prefWidth="150.0" promptText="menu names" />
                                <TextField fx:id="gras1" layoutX="67.0" layoutY="136.0" text="fat" />
                                <TextField fx:id="fr1" layoutX="67.0" layoutY="248.0" text="fruit" />
                                <TextField fx:id="leg1" layoutX="67.0" layoutY="336.0" text="vegetables" />
                                <TextField fx:id="cereal" layoutX="67.0" layoutY="428.0" text="cereals products" />
                                <TextField fx:id="gras2" layoutX="355.0" layoutY="136.0" text="fat" />
                                <TextField fx:id="fr2" layoutX="355.0" layoutY="248.0" text="fruit" />
                                <TextField fx:id="leg2" layoutX="355.0" layoutY="336.0" text="vegetables" />
                                <TextField fx:id="cereal1" layoutX="355.0" layoutY="428.0" text="cereals products" />
                                <TextField fx:id="boi" layoutX="639.0" layoutY="136.0" text="drink" />
                                <TextField fx:id="fr3" layoutX="647.0" layoutY="248.0" text="fruit" />
                                <TextField fx:id="leg3" layoutX="647.0" layoutY="336.0" text="vegetables" />
                                <TextField fx:id="vvpolav" layoutX="647.0" layoutY="428.0" text="vvpolav" />
                            </children></AnchorPane>
                    </content>
                </Tab>
                <Tab closable="false" text="Gestion des Achats">
                    <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="600.0" prefWidth="900.0">
                            <children>
                                <TableView fx:id="StorageTable" layoutX="10.0" layoutY="14.0" onMouseClicked="#getSelected" onMouseEntered="#getSelected" prefHeight="410.0" prefWidth="850.0">
                                    <columns>
                                        <TableColumn fx:id="StrorageIDElement" prefWidth="170.0" text="idelement" />
                                        <TableColumn fx:id="StrorageElement" prefWidth="180.0" text="element" />
                                        <TableColumn fx:id="StorageZone" prefWidth="243.0" text="storage zone" />
                                        <TableColumn fx:id="StorageType" prefWidth="256.0" text="storage type" />
                                    </columns>
                                </TableView>
                                <MenuButton fx:id="MenuZone" layoutX="310.0" layoutY="462.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="106.0" text="Zone">
                                    <items>
                                        <RadioMenuItem fx:id="ZoneA" mnemonicParsing="false" text="zoneA" />
                                        <RadioMenuItem fx:id="ZoneB" mnemonicParsing="false" text="zoneB" />
                                    </items>
                                </MenuButton>
                                <MenuButton fx:id="MenuType" layoutX="613.0" layoutY="462.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="106.0" text="Type">
                                    <items>
                                        <RadioMenuItem fx:id="TypeFroid" mnemonicParsing="false" text="froid" />
                                        <RadioMenuItem fx:id="TypeHumide" mnemonicParsing="false" text="humide" />
                                    </items>
                                </MenuButton>
                                <Button fx:id="StorageAddElement" layoutX="340.0" layoutY="514.0" mnemonicParsing="false" text="Add element" />
                                <Button fx:id="StorageModifyButton" layoutX="471.0" layoutY="514.0" mnemonicParsing="false" text="Modify" />
                                <Button fx:id="StorageDeleteButton" layoutX="252.0" layoutY="514.0" mnemonicParsing="false" text="Delete" />
                                <TextField fx:id="TextidElement" layoutX="28.0" layoutY="462.0" prefHeight="26.0" prefWidth="106.0" text="id element" />
                                <TextField fx:id="Textelement" layoutX="154.0" layoutY="463.0" prefHeight="26.0" prefWidth="104.0" text="element" />
                                <TextField fx:id="Textstoragezone" layoutX="435.0" layoutY="463.0" text="zone" />
                                <TextField fx:id="Textstoragetype" layoutX="737.0" layoutY="464.0" prefHeight="26.0" prefWidth="100.0" text="type" />
                            </children>
                        </AnchorPane>
                    </content>
                </Tab>
                <Tab closable="false" text="Gestion des commandes">
                    <content>
                        <AnchorPane prefHeight="600.0" prefWidth="900.0" style="-fx-background-color: #FFFFFF;">
                            <children>
                                <Label layoutX="27.0" layoutY="14.0" prefHeight="34.0" prefWidth="134.0" text="New Order">
                                    <font>
                                        <Font name="Arial" size="18.0" />
                                    </font>
                                </Label>
                                <DatePicker fx:id="ChefOrderDate" editable="false" layoutX="125.0" layoutY="101.0" prefHeight="25.0" prefWidth="195.0" promptText="order date" />
                                <DatePicker fx:id="ChefReceptionDate" editable="false" layoutX="125.0" layoutY="142.0" prefHeight="26.0" prefWidth="195.0" promptText="reception date" />
                                <TextField fx:id="ChefQuantity" layoutX="124.0" layoutY="230.0" prefHeight="26.0" prefWidth="196.0" promptText="Quantity" />
                                <Button fx:id="ChefSubmit" layoutX="258.0" layoutY="334.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="282.0" style="-fx-background-color: #abc1be;" text="submit" />
                                <Line endX="417.0" layoutX="282.0" startX="-100.0" />
                        <ComboBox fx:id="ChefOrder" layoutX="125.0" layoutY="187.0" prefHeight="26.0" prefWidth="191.0" promptText="order" />
                            </children>
                        </AnchorPane>
                    </content>
                </Tab>
                <Tab text="gestions de provisions">
                    <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                            <children>
                                <ComboBox fx:id="MenuButton" layoutX="374.0" layoutY="64.0" prefWidth="150.0" promptText="elements" />
                                <TextField fx:id="InitialQuantity" layoutX="352.0" layoutY="166.0" prefHeight="26.0" prefWidth="194.0" />
                                <TextField fx:id="ConsumedQuantity" layoutX="352.0" layoutY="258.0" prefHeight="26.0" prefWidth="194.0" />
                                <Button fx:id="RefreshButton" layoutX="375.0" layoutY="429.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="149.0" text="submit" />
